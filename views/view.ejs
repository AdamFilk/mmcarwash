<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.22.2/moment.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/tempusdominus-bootstrap-4/5.0.1/js/tempusdominus-bootstrap-4.min.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/tempusdominus-bootstrap-4/5.0.1/css/tempusdominus-bootstrap-4.min.css" />
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
    <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
    <title>View Booking </title>

</head>
<body>
  <script>
    (function(d, s, id){
    var js, fjs = d.getElementsByTagName(s)[0];
    if (d.getElementById(id)) {return;}
    js = d.createElement(s); js.id = id;
    js.src = "//connect.facebook.net/en_US/messenger.Extensions.js";
    fjs.parentNode.insertBefore(js, fjs);
  }(document, 'script', 'Messenger'));
  const APP_ID = 711149509404832;    
  var senderID = "";
  
  window.extAsyncInit = function() {
      MessengerExtensions.getContext(APP_ID, 
        function success(thread_context){
         senderID = thread_context.psid;
        },
        function error(err){
          console.log(err);
        }
      );        
  };
</script>
    <div class="container-fluid">
        <div class="row justify-content-md-center">
            <div class="col-sm-4 w-100">
              
                <table class="table table-bordered">
                    <thead>
                        <tr>
                            <td colspan="2" class="text-center"><h2>Booking Review</h2></td>
                        </tr>
                    </thead>
                    <tr>
                        <td>
                          <b>Name:</b>
                        </td>
                        <td>
                            <input type="text"  id="Name" name="Name" value="<%= data.Name %>">
                               
                        </td>
                    </tr>
                    <tr>
                        <td style="vertical-align: middle;"><b>Phone:</b> </td>
                        <td>
                            <div class="input-group mb-3">
                                <div class="input-group-prepend">
                                  <span class="input-group-text" id="basic-addon1">+95</span>
                                </div>
                                <input  type="text" class="form-control" aria-label="phone_number" aria-describedby="basic-addon1" value="<%= data.phone %>">
                              </div>
                        </td>
                    </tr>
                    
                    <tr>
                      <td>
                        <b>City:</b>
                      </td>
                      <td>
                        <input class="form-control" type="text" placeholder="Only in Yangon for now" id="city" readonly>
                      </td>
                    </tr>
                    
                    <tr>
                        <td style="vertical-align: middle;"><b>Town:</b> </td>
                        <td>    
                            <input type="text" class="form-control" value="<%= data.town %>">
                        </td>
                    </tr>   
                    
                    <tr>
                        <td>
                           <b>Address Information</b> 
                        </td>
                        <td>
                          <input type="text" class="form-control" value="<%= data.address %>">
                        </td>
                    </tr>

                    <tr>
                        <td>
                          Car Plate
                        </td>
                        <td>
                          <input type="text" class="form-control" name="car_plate" id="car_plate" value="<%= data.carpalte %>">
                        </td>
                    </tr>
                    <tr>
                      <td>
                        Car Brand
                      </td>
                      <td>
                        <input type="text" class="form-control" name="car_brand" id="car_brand" value="<%= data.carbrand %>">
                      </td>
                  </tr>
                  <tr>
                    <td>
                      Car Model
                    </td>
                    <td>
                        <input type="text" class="form-control" name="car_model" id="car_model" value="<%= data.carmodel %>">
                    </td>
                </tr>
                    
                    <tr>
                    <td>
                      <b>Car Size</b>
                    </td>
                    <td>
                        <input type="text" class="form-control" name="carsize" id="carsize" value="<%= data.carsize %>">
                    </td>
                    </tr>
                    <tr>
                      <td>
                        Pet Hair
                      </td>
                      <td> 
                        <input type='text' name="add_on0" id='add_on0' value="<%= data.pethair %>"  > 
                    </td>
                    </tr>
                    <tr>
                        <td>
                          Wax
                        </td>
                        <td> 
                          <input type='text' name="add_on1" id='add_on' value="<%= data.wax %>"  > 
                      </td>
                      </tr>
                      <tr>
                        <td>
                         Scratch Removal
                        </td>
                        <td> 
                          <input type='text' name="add_on2" id='add_on2' value="<%= data.scratch %>"  > 
                      </td>
                      </tr>
                      <tr>
                        <td>
                         Clay Bar
                        </td>
                        <td> 
                          <input type='text' name="add_on3" id='add_on3' value="<%= data.claybar %>"  > 
                      </td>
                      </tr>
                      <tr>
                        <td>
                         Tire and alloy cleaning
                        </td>
                        <td> 
                          <input type='text' name="add_on4" id='add_on4' value="<%= data.tire_alloy %>"  > 
                      </td>
                      </tr>
                    <tr>
                      <td>
                       <b>Total Price:</b> 
                      </td>
                      <td>

                        <input type="text" name="total" id="total" value="<%= data.total_price %>">
                      </td>
                    </tr>
                    
                      <tr>
                        <td><b>Date</b> </td>
                        <td>
                                
                            <input class="form-control" type="date" value="<%= data.date %>" id="date-input" name="date-input" required>
                              
                        </td>
                    </tr>
                    
                    <tr>
                      <td>
                        <b>Time</b> 
                     </td>
                     <td>
                         <input class="form-control" type="time" value="<%= data.time %>" id="time-input" name="time-input" required>
                     </td>
                    </tr>
                    
                   
                            <input type="hidden"  id="sender" name="sender" value="<%= data.id %>">
                          
                       
                    <tr>
                        
                    </tr>
                    <tr>
                        <td>
                            Wash Type
                        </td>
                        <td>
                            <input type="text"  id="wash_type" name="wash_type" value="<%= data.wash_type %>">
                        </td>
                    </tr>
                    <tr>
                        <td>
                            Interior or Exterior
                        </td>
                         <td>
                             <input type="text"  id="int_ext" name="int_ext" value="<%= data.Interior_or_Exterior %>">
                         </td>
                    </tr>
                    <tr>
                        <td>
                            <button type="button" class="btn btn-primary" onclick="myFunction()">Close</button>
                        </td>
                    </tr>
                </table>  
            </div>
        </div>
    </div>
    
</body>
<script>
  function  myFunction(){
    MessengerExtensions.requestCloseBrowser(function success() {
      console.log("success");          
    }, function error(err) {
      console.log(err);
    });
    }
</script>
</html>